{
  "name": "@tokenlay/sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "JavaScript SDK for Tokenlay - AI operations platform with intelligent routing and cost control",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "prepublishOnly": "npm run build && npm run test && npm run typecheck",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "npm publish",
    "postrelease": "git push --tags"
  },
  "keywords": [
    "ai",
    "openai",
    "llm",
    "proxy",
    "rate-limiting",
    "usage-tracking",
    "cost-control",
    "business-logic",
    "tokenlay"
  ],
  "author": "Tokenlay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokenlay/tokenlay-sdk-js.git"
  },
  "homepage": "https://tokenlay.com",
  "bugs": {
    "url": "https://github.com/tokenlay/tokenlay-sdk-js/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "openai": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
